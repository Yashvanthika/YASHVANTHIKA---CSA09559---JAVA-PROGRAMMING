package p;

public class Linear {
	public static double factorial(double d)
	{
		if(d<=1)
		{
			return 1;
		}
		else 
		{
		   return d*factorial(d-1);
		}
	}
	public static void main(String[] args) 
	{
		double d=7;
		factorial(d);
		System.out.println("factorial of "+d+" is "+factorial(d));
	}

}
